 -->  <h1 class="page_header">Sign up!</h1>
  <%= form_for(@user, url: signup_path, html:{class: "form"}) do |f| %>
    <div class="form_group">
      <%= f.label(:username, class: 'form-label') %>
      <%= f.text_field(:username, class: 'form-control') %>
    </div></br>
     <div class="form_group">
      <%= f.label(:password, class: 'form-label') %>
      <%= f.password_field(:password, class: 'form-control') %>
    </div></br>
     <div class="form_group">
      <%= f.label(:password_confirmation, class: 'form-label') %>
      <%= f.password_field(:password_confirmation, class: 'form-control') %>
    </div></br>
     <div class="form_group">
      <%= f.label(:gender, class: 'form-label') %>
      <%= f.select(:gender, options_for_select(['male', 'female'])) %>
    </div></br> 
    <div class="form_group">
    <%= f.label(:age, class: 'form-label') %>
    <%= f.text_field(:age, class: 'form-control') %>
    </div></br>
    <div class="form_group">
      <%= f.label(:bio, class: 'form-label') %>
      <%= f.text_field(:bio, class: 'form-control') %>
    </div></br>
    <div class="form_group">
      <%= f.label(:contact, class: 'form-label') %>
      <%= f.text_field(:contact, class: 'form-control') %>
    </div></br>
     <div class="form_group">
      <%= f.submit('Signup', class: 'btn btn-lg btn-success') %>
    </div>
  <% end %>
